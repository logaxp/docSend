{
  "name": "docsend",
  "version": "1.0.0",
  "description": "Electronic document management system",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "env NODE_ENV=development nodemon app",
    "migrate:up": "npx sequelize-cli db:migrate --url mysql://root:@127.0.0.1:3306/doc_send_db",
    "migration:undo": "npx sequelize-cli db:migrate:undo --url mysql://root:@127.0.0.1:3306/doc_send_db",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all --url mysql://root:@127.0.0.1:3306/doc_send_db",
    "seed:all": "npx sequelize-cli db:seed:all --url mysql://root:@127.0.0.1:3306/doc_send_db",
    "create:seed:roles": "npx sequelize-cli seed:generate --name roles",
    "create:seed:super": "npx sequelize-cli seed:generate --name super-admin-user"
  },
  "keywords": [
    "docsend"
  ],
  "author": "Gabriel J.",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "flatted": "^3.2.9",
    "helmet": "^7.1.0",
    "html-pdf": "^3.0.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^21.5.2",
    "randomstring": "^1.3.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1"
  }
}
